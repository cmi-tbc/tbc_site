{
  "rewrites": [],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ],
  "redirects": [
    {
      "source": "/assessment_results.html",
      "destination": "/results",
      "permanent": true
    },
    {
      "source": "/burnout_results_page_rebuild_2025-06-03.html",
      "destination": "/results", 
      "permanent": true
    }
  ]
}